@model IEnumerable<MentalHealthApp.Models.CustomUser>

<h2>User List</h2>
<table class="table">
    <thead>
        <tr>
            <th>Email</th>
            <th>Actions</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var user in Model)
        {
            <tr>
                <td>@user.Email</td>
                <td>
                    <a asp-action="ManageRole" asp-route-userId="@user.Id">Manage Roles</a>
                </td>
            </tr>
        }
    </tbody>
</table>